<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name=" CLOUD BITBAR - Web Automation UI" >

    <listeners>
                <listener class-name="commons.MethodListener"></listener>
    </listeners>

    <parameter name="server" value="dev" />

    <test name="Run on Firefox browser">
        <parameter name="os" value="linux"></parameter>
        <parameter name="os_version" value="18.04"></parameter>
        <parameter name="browser" value="firefox"></parameter>
        <parameter name="browser_version" value="130"></parameter>
        <classes>
            <class name="com.nopcommerce.users.Level_36_CloudTest_Bitbar" />
        </classes>
    </test>

    <test name="Run on Chrome browser">
        <parameter name="os" value="Windows"></parameter>
        <parameter name="os_version" value="10"></parameter>
        <parameter name="browser" value="chrome"></parameter>
        <parameter name="browser_version" value="latest"></parameter>
        <classes>
            <class name="com.nopcommerce.users.Level_36_CloudTest_Bitbar" />
        </classes>
    </test>


</suite>